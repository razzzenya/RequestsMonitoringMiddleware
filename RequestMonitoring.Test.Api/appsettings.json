{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OpenSearch": {
    "Uri": "http://192.168.0.101:9200",
    "Index": "request-logs"
  },
  "ConnectionStrings": {
    "Default": "Data Source=domains.db"
  },
  "CacheSettings": {
    "ExpirationMinutes": 25
  },
  "AllowedHosts": "*"
}
